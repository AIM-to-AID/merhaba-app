{
  "expo": {
    "name": "Merhaba - a hub for refugees",
    "slug": "merhaba-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/meta/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/meta/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/meta/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "bundle.ios.aimtoaid"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/meta/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA8V9tV4JaDrfuZxym92gGu44pa_GfR2DE"
        }
      },
      "versionCode": 2,
      "package": "bundle.android.aimtoaid"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Merhaba to use your location to find nearby locations."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "00f5434a-e44c-4ab2-9c10-bc45370d8562"
      }
    },
    "owner": "aimtoaid_developer"
  }
}
